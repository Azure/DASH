{% if table.enable_bulk_get_api == 'true' %}
/**
 * @brief Bulk Get Op filter keywords for {{ table.name }} in get_{{ table.name | replace("entry", "entries")}}_attribute call
 */
typedef enum _sai_{{ table.name }}_bulk_get_filter_t
{
{% for key in table['keys'] %}
    /** Bulk get filter key word for {{ key.type }} {{ key.name | lower }} */
    SAI_{{ table.name | upper }}_BULK_GET_FILTER_T_{{ key.name | upper }},

{% endfor %}
{% for sai_attr in table.sai_attributes %}
    /** Bulk get filter key word for SAI_{{ table.name | upper }}_ATTR_{{ sai_attr.name | upper }} */
    SAI_{{ table.name | upper }}_BULK_GET_FILTER_T_{{  sai_attr.name  | upper }},

{% endfor %}
} sai_{{ table.name }}_bulk_get_filter_t;

/**
 * @brief Bulk Get Op for {{ table.name }} in get_{{ table.name | replace("entry", "entries")}}_attribute call
 */
typedef enum _sai_{{ table.name }}_bulk_get_op_t
{
    /** Indicate the last operation of the bulk get */
    SAI_{{ table.name | upper }}_BULK_GET_OP_LAST_ITEM,

    /** Operation to compare the value is equal */
    SAI_{{ table.name | upper }}_BULK_GET_OP_EQUAL_TO,

    /** Operation to compare the value is greater than */
    SAI_{{ table.name | upper }}_BULK_GET_OP_GREATER_THAN,

    /** Operation to compare the value is greater than or equal to */
    SAI_{{ table.name | upper }}_BULK_GET_OP_GREATER_THAN_OR_EQUAL_TO,

    /** Operation to compare the value is less than */
    SAI_{{ table.name | upper }}_BULK_GET_OP_LESS_THAN,

    /** Operation to compare the value is less than or equal to */
    SAI_{{ table.name | upper }}_BULK_GET_OP_LESS_THAN_OR_EQUAL_TO,

} sai_{{ table.name }}_bulk_get_op_t;

typedef struct _sai_{{ table.name }}_target_server_data_t
{
    sai_ip_address_t server_ip;
    sai_uint16_t server_port;
} sai_{{ table.name }}_target_server_data_t;

typedef struct _sai_flow_entry_filter_data_t
{
    sai_{{ table.name }}_bulk_get_filter_t filter;
    sai_{{ table.name }}_bulk_get_op_t op;
    sai_uint64_t value;
} sai_{{ table.name }}_filter_data_t;

{% endif %}